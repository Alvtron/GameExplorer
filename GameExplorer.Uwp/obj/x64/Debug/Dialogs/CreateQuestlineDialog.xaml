<ContentDialog x:ConnectionId='1'
    x:Class="GameExplorer.Uwp.Dialogs.CreateQuestLineDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:GameExplorer.Uwp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Create Questline"
    PrimaryButtonText="Cancel"
    SecondaryButtonText="Create"
                                                         
                                                             
                                    >

    <StackPanel>
        <TextBox x:ConnectionId='2' Header="Title" Name="TitleBox" Text="{Binding Title, Mode=TwoWay}"                                   />
        <AutoSuggestBox x:ConnectionId='3' Name="GameASB" Header="Game"                                                                                                                                   />
    </StackPanel>
</ContentDialog>

